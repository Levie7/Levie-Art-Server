{
    "version": 2,
    "builds": [
      {
        "src": "api/**/*.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
        {
          "src": "/api/portfolio",
          "dest": "api/portfolio.ts"
        },
        {
            "src": "/api/uploadImage",
            "dest": "api/uploadImage.ts"
        }
    ]
  }
  